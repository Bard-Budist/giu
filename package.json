{
  "name": "giu",
  "version": "0.2.0",
  "description": "A collection of React components and utilities",
  "main": "lib/index.js",
  "author": "Guillermo Grau Panea",
  "license": "MIT",
  "keywords": [
    "React",
    "components",
    "collection",
    "forms",
    "inputs"
  ],
  "homepage": "https://github.com/guigrpa/giu#readme",
  "bugs": {
    "url": "https://github.com/guigrpa/giu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guigrpa/giu.git"
  },
  "scripts": {
    "start": "babel-node examples/server",
    "compile": "rm -rf ./lib ./libEs6 ./libEs6_flow && babel -d lib src && cp src/index.sass lib",
    "docs": "extract-docs --template docs/templates/README.md --output README.md",
    "build": "npm run lint && npm run compile && npm run docs",
    "travis": "npm run compile",
    "lint": "eslint src",
    "flow": "flow && test $? -eq 0 -o $? -eq 2",
    "flowStop": "flow stop",
    "test": "npm run testCovFull",
    "testCovFull": "npm run testCovPrepare && npm run testDev && npm run testProd && npm run testCovReport",
    "testCovFast": "npm run testCovPrepare && npm run testDev && npm run testCovReport",
    "ava": "ava --watch",
    "testCovPrepare": "rm -rf ./coverage .nyc_output .nyc_tmp && mkdir .nyc_tmp",
    "testDev": "cross-env NODE_ENV=development nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testProd": "cross-env NODE_ENV=production nyc ava && mv .nyc_output/* .nyc_tmp/",
    "testCovReport": "cp .nyc_tmp/* .nyc_output/ && nyc report --reporter=html --reporter=lcov --reporter=text"
  },
  "engines": {
    "node": ">=4"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-addons-pure-render-mixin": ">=0.14.0",
    "moment": "^2.0.0"
  },
  "dependencies": {
    "filesize": "3.3.0",
    "font-awesome": "4.6.1",
    "keycode": "2.1.1",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "timm": "^0.6.1",
    "tinycolor2": "1.3.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "0.5.4",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-flowtype": "^2.2.2",
    "eslint-plugin-react": "^4.2.3",
    "extract-docs": "^1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "flow-bin": "^0.22.1",
    "json-loader": "0.5.4",
    "moment": "^2.0.0",
    "node-sass": "3.4.2",
    "nyc": "^6.1.1",
    "react": ">=0.14.0",
    "react-addons-perf": ">=0.14.0",
    "react-addons-pure-render-mixin": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "sass-loader": "3.2.0",
    "storyboard": "^1.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "babel": "inherit"
  }
}
